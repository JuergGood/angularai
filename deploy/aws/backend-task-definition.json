{
    "family":  "angularai-backend",
    "networkMode":  "awsvpc",
    "containerDefinitions":  [
                                 {
                                     "name":  "backend",
                                     "image":  "426141506813.dkr.ecr.eu-central-1.amazonaws.com/angularai-backend:$NewVersion",
                                     "essential":  true,
                                     "portMappings":  [
                                                          {
                                                              "containerPort":  8080,
                                                              "hostPort":  8080,
                                                              "protocol":  "tcp"
                                                          }
                                                      ],
                                     "environment":  [
                                                         {
                                                             "name":  "SPRING_PROFILES_ACTIVE",
                                                             "value":  "h2-mem"
                                                         },
                                                         {
                                                             "name":  "IPSTACK_API_KEY",
                                                             "value":  "PLACEHOLDER_IPSTACK_API_KEY"
                                                         }
                                                     ],
                                     "secrets": [
                                         {
                                             "name": "ADMIN_PASSWORD",
                                             "valueFrom": "arn:aws:secretsmanager:eu-central-1:426141506813:secret:goodone-config-NCy6aP:ADMIN_PASSWORD::"
                                         },
                                         {
                                             "name": "USER_PASSWORD",
                                             "valueFrom": "arn:aws:secretsmanager:eu-central-1:426141506813:secret:goodone-config-NCy6aP:USER_PASSWORD::"
                                         },
                                         {
                                             "name": "ADMIN_READ_PASSWORD",
                                             "valueFrom": "arn:aws:secretsmanager:eu-central-1:426141506813:secret:goodone-config-NCy6aP:ADMIN_READ_PASSWORD::"
                                         },
                                         {
                                             "name": "ADMIN_EMAIL",
                                             "valueFrom": "arn:aws:secretsmanager:eu-central-1:426141506813:secret:goodone-config-NCy6aP:ADMIN_EMAIL::"
                                         },
                                         {
                                             "name": "USER_EMAIL",
                                             "valueFrom": "arn:aws:secretsmanager:eu-central-1:426141506813:secret:goodone-config-NCy6aP:USER_EMAIL::"
                                         },
                                         {
                                             "name": "ADMIN_READ_EMAIL",
                                             "valueFrom": "arn:aws:secretsmanager:eu-central-1:426141506813:secret:goodone-config-NCy6aP:ADMIN_READ_EMAIL::"
                                         }
                                     ],
                                     "logConfiguration":  {
                                                              "logDriver":  "awslogs",
                                                              "options":  {
                                                                              "awslogs-group":  "/ecs/angularai-backend",
                                                                              "awslogs-region":  "eu-central-1",
                                                                              "awslogs-stream-prefix":  "ecs"
                                                                          }
                                                          }
                                 }
                             ],
    "requiresCompatibilities":  [
                                    "FARGATE"
                                ],
    "cpu":  "256",
    "memory":  "512",
    "executionRoleArn":  "arn:aws:iam::426141506813:role/ecsTaskExecutionRole",
    "taskRoleArn":  "arn:aws:iam::426141506813:role/ecsTaskExecutionRole"
}
